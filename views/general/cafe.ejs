<%include ../partials/header %>

    <div class="container main_container">

        <header class="jumbotron">
            <div class="container">
                <h1>
                    Sunshine Cafe
                </h1>

                <p>
                    Food and drink from fresh organic products
                </p>
            </div>

        </header>
		
		
		<div class = "well well-sm text-center" >
			<h3>
				All foods and drinks contain healthy level of heart-shaped herb extract.
			</h3>
		</div>
		

        
            <%var alternate = true;%>
                <% home_image.forEach(function(farm_image){ %>
                    <% if (farm_image.tag == "cafe"){ %>
                        <% if (alternate == true ){ %>
							<div class="container">
							<div class = "space_add"></div>
                            <div class="row">
                				<div class="col-lg-4 col-md-6 col-sm-6" >
                                    <img src="<%= farm_image.image %>" class = "img_cafe">
                                </div>
                				<div div class="col-lg-8 col-md-6 col-sm-6 text_bg_cafe">
                    				<h4 class = "text_cafe">
									<%= farm_image.name %>	
									</h4>
                				</div>
							</div>
            				</div>
                    
                        <% } else {%>
							<div class="container">
								<div class = "space_add"></div>
                            <div class="row" >
                				
                				<div div class="col-lg-8 col-md-6 col-sm-6 text_bg_cafe">
                    				<h4 class = "text_cafe">
										<%= farm_image.name %>
									</h4>
                				</div>
								<div class="col-lg-4 col-md-6 col-sm-6">
                                    <img src="<%= farm_image.image %>"  class="img-rounded img_cafe">
                                </div>
							</div>
            				</div>
							
						<% } %>
						<% alternate = !alternate; %>
                    <% } %>
                <% }); %>
				<div class = "space_add" ></div>
				<div class = "space_add" ></div>
		</div>			

        <%include ../partials/footer %>